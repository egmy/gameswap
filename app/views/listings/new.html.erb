<h1>Add New Listing</h1>
<%= bootstrap_form_with model: @listing, method: :post, url: game_path(@game), data: { turbo: false } do |f| %>
  <%= f.text_field :game %>
  <%= f.text_field :description %>
  <%= f.submit %>
<% end %>
<p class="mt-3">
  <%= link_to 'Cancel', game_path(@game), class: 'btn btn-outline-secondary' %>
</p>